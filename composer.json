{
    "name": "horisen/cms-skeleton",
    "description": "Horisen CMS Skeleton app",
    "require": {
	"horisen/installers": "*",
	"horisen/cms-core-mod": "*",
	"horisen/cms-teaser-mod": "*",
	"horisen/cms-tinymce-plugin": "*"
    },
    "minimum-stability": "dev",
    "repositories": [ {"type": "composer", "url": "http://wa.cms.satis/"} ],
    "autoload": {
        "psr-0": {
            "Hcms\\": "src/"
        }
    },
    "scripts": {
        "pre-install-cmd": "Hcms\\Installer::preInstall",
        "post-install-cmd": "Hcms\\Installer::postInstall",
        "post-package-install": "Hcms\\Installer::postPackageInstall"
    }
}
